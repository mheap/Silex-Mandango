            "type": "package",
            "package": {
                "name": "mandango/mondator",
                "version": "master",
                "dist": {
                    "url": "https://github.com/mandango/mondator/zipball/5685054409a76b41c27573e4bc17523f8236551a",
                    "type": "zip"
                },
                "source": {
