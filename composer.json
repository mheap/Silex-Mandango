                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "mandango/mandango",
                "version": "master",
                "dist": {
                    "url": "https://github.com/mandango/mandango/zipball/957d6c6f54c8ed0762ab63b16a057d1e631bc002",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/mandango/mandango.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": {
                        "Mandango\\Mandango": "src"
                    }
                },
                "require": {
                    "symfony/class-loader": ">=2.0.0",
                    "twig/twig": ">=1.2",
                    "mandango/mondator": "*"
